{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts",
      "method": ["GET", "POST", "PATCH", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.ts",
      "method": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
